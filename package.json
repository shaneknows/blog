{
  "name": "gh-pages",
  "author": "shaneknows",
  "version": "1.0.1",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "build:content": "processmd content/**/*.md --outputDir content --preview 110 --summaryOutput content/summary.json",
    "start": "next start",
    "export": "next export",
    "deploy": "next build && next export && touch out/.nojekyll && git add out/ && git commit -m \"Deploy to gh-pages\" && git subtree push --prefix out origin gh-pages"
  },
  "dependencies": {
    "next": "latest",
    "processmd": "^4.4.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "license": "ISC",
  "devDependencies": {
    "babel-plugin-transform-define": "^1.3.1"
  }
}
